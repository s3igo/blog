---
import Card from "../atoms/Card.astro";
import type { Card as CardType } from "~/types";

type Props = {
    cards: CardType[];
};
const { cards } = Astro.props;
---

<main>
    {
        cards.map((card) => (
            <div class="py-5">
                <Card {...card} />
            </div>
        ))
    }
</main>
