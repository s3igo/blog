---
import Icon from './Header/Icon.astro';
import { Navbar, type Props as NavbarProps } from './Header/Navbar';

export type Props = NavbarProps;
const props = Astro.props;
---

<header class="flex gap-4 sm:gap-5">
    <div>
        <Icon />
    </div>
    <div class="w-full">
        <Navbar {...props} />
    </div>
</header>
